{
  "name": "v_core_timers",
  "version": "0.1.3",
  "description": "Simple to use Timers with Events for node and web.",
  "main": "./dist/v_core_timers.cjs.js",
  "module": "./dist/v_core_timers.es.js",
  "jsnext:main": "./dist/v_core_timers.cjs.js",
  "scripts": {
    "export:types": "sh -c \"npx -p typescript tsc ./source/*.js --declaration --allowJs --emitDeclarationOnly --outDir types\"",
    "test": "jest --runInBand",
    "test:watch": "jest --runInBand --watch",
    "build": "rollup -c ./rollup.config.js --bundleConfigAsCjs",
    "build:prod": "rollup -c ./rollup.config.js --bundleConfigAsCjs --environment NODE_ENV:production",
    "lint:fix": "eslint source --fix",
    "lint": "eslint source",
    "prepack": "npm run lint && npm run export:types && npm run build:prod && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/v-core9/v_core_timers.git"
  },
  "keywords": [
    "v-core9",
    "interval",
    "timers",
    "setInterval",
    "events",
    "EventEmitter"
  ],
  "author": {
    "name": "Slavko Vuletic",
    "url": "https://github.com/v-core9"
  },
  "license": "The Unlicense",
  "bugs": {
    "url": "https://github.com/v-core9/v_core_timers/issues"
  },
  "homepage": "https://github.com/v-core9/v_core_timers#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-strip": "^3.0.4",
    "@rollup/plugin-terser": "^0.4.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "jest": "^29.5.0",
    "rollup": "^4.9.0",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "typescript": "^5.3.3"
  },
  "types": "./types/index.d.ts",
  "typings": "./types/index.d.ts"
}